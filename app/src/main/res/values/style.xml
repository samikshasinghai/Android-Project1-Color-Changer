<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="RedSB" parent="Widget.AppCompat.SeekBar">
        <item name="android:colorControlActivated">#ff0000</item>
        <item name="android:progressTint">#ff0000</item>
        <item name="android:progressBackgroundTint">#000</item>
    </style>

    <style name="GreenSB" parent="Widget.AppCompat.SeekBar">
        <item name="android:colorControlActivated">#00ff00</item>
        <item name="android:progressTint">#00ff00</item>
        <item name="android:progressBackgroundTint">#000</item>
    </style>

    <style name="BlueSB" parent="Widget.AppCompat.SeekBar">
        <item name="android:progressBackgroundTint">#000</item>
        <item name="android:progressTint">#0000ff</item>
        <item name="android:colorControlActivated">#0000ff</item>
    </style>

    <style name="tvStyle" parent="TextAppearance.AppCompat">
        <item name="android:textSize">20sp</item>
        <item name="android:textColor">#D2691E</item>
    </style>

    <style name="CheckBoxStyle" parent="Theme.AppCompat.Light">
        <item name="colorAccent">@android:color/holo_red_light</item>
        <item name="android:textColorSecondary">@android:color/holo_blue_bright</item>
    </style>

    <style name="RadioButtonStyle" parent="Theme.AppCompat.Light">
        <item name="colorAccent">@android:color/holo_red_light</item>
        <item name="android:textColorSecondary">@android:color/holo_green_light</item>
    </style>


</resources>